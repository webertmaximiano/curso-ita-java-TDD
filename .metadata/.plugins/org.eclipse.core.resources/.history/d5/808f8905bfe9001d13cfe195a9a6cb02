
import java.util.List;

public class ConversorDePalavras {

	public static String converterCamelCase(List<String> original) {
	    
	    
	    StringBuilder convertida = new StringBuilder();
	    
	    for (int i = 0; i < original.size(); i++) {
	        String palavra = original.get(i);
	        if (i == 0) {
	            convertida.append(palavra.toLowerCase());
	        } else {
	            convertida.append(palavra.substring(0, 1).toUpperCase());
	            convertida.append(palavra.substring(1).toLowerCase());
	        }
	    }
	    
	    return convertida.toString();
	}

}
