import static org.junit.Assert.*;
import java.util.Arrays;
import java.util.List;

import org.junit.Test;

public class TesteCamelCase {
    private ConversorDePalavras c;
    private List<String> palavras;

    @Test
    public void converterCamelCase() {
        c = new ConversorDePalavras();
        palavras = Arrays.asList("webert");
        String palavraConvertida = c.converterCamelCase(palavras);
        assertEquals("Webert", palavraConvertida);
    }
}